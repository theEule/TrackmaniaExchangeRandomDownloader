        function switchScreenshot(position = 0) {
            if (timeout)
                return;
            timeout = true;
            $('#screenshotToggleText').fadeOut(150);
            if (position != 0)
                cur = position;
            $('#screenshot').fadeOut(150, function () {
               $('#screenshot').attr('src', '/tracks/' + (position != 0 ? '54950' + '/image/' + position + '?dummy=' + date.getTime() : 'thumbnail/' + '54950'));
