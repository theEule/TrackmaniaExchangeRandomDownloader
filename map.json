                $('a[id^="image-"]').removeClass('active');
                $('#image-' + position).addClass('active');
                $('#screenshotToggleText').fadeIn(150);
                $('#screenshot').fadeIn(150, function () {
                    timeout = false;
                });
            });
        }

        var madeVoteDetailsRequest = false;
        function toggleVoteContainer() {
            if (!madeVoteDetailsRequest) {
                madeVoteDetailsRequest = true;
                $('#voteDetailsToggler').html('<i class="icon-collapse"></i> Obtaining data...');
                $.get('https://karma.mania-exchange.com/apihtml/mapdetailedinfo?uid=PWNN8JArfLNPfZtu30yTNP5pZQ3', function (data) {
                    $('#voteDetailsContainer').html(data);
                    $('#voteDetailsContainer').slideDown();
                    $('#voteDetailsToggler').html('<i class="icon-collapse-top"></i> Hide rating details');
                });
            } else {
                if ($('#voteDetailsContainer').is(':visible')) {
                    $('#voteDetailsContainer').slideUp();
                    $('#voteDetailsToggler').html('<i class="icon-collapse"></i> Show rating details');
                } else {
                    $('#voteDetailsContainer').slideDown();
                    $('#voteDetailsToggler').html('<i class="icon-collapse-top"></i> Hide rating details');
                }
            }
        }

        let awardButton = $('#awardButton');
        let awardList = $('#awardList');
        let awardListToggle = $('#awardListToggle');
        let awardCount = $('#awardCount');
        let hideList = $('.hideAwardList');

        let awardData = null;
        let selfAward = null;

        function loadAwards() {
            $(".hideAwardList").toggle();
            setLoading($('#awardMaster'), false);
            setLoading($('#awardPanel'), true);
            awardData = null;
            $.get('/api/awards/33755', function (data) {
                awardData = data;
                awardCount.text(data.length);
                let awardListPreview = $('<a href="javascript:;" title="View all users that awarded this map"></span>');
                if (data.length == 0) {
                    awardListPreview = $('<span>No-one awarded this map yet.</span>')
                } else if (data.length == 1) {
                    awardListPreview.append($('<span></span>').text(data[0].UserLink.Username));
                    awardListPreview.append('&nbsp;awarded this map!');
                } else if (data.length == 2) {
                    awardListPreview.append($('<span></span>').text(data[1].UserLink.Username));
                    awardListPreview.append('&nbsp;and&nbsp;');
                    awardListPreview.append($('<span></span>').text(data[0].UserLink.Username));
                    awardListPreview.append('&nbsp;awarded this map!');
                } else {
                    awardListPreview.append($('<span></span>').text(data[data.length - 1].UserLink.Username));
                    awardListPreview.append(',&nbsp;');
                    awardListPreview.append($('<span></span>').text(data[data.length - 2].UserLink.Username));
                    awardListPreview.append('&nbsp;and ' + (data.length - 2) + ' others awarded this map!');
                }

                awardList.html('').append(awardListPreview);
                awardListPreview.on('click', showFullAwardList);
                awardListToggle.attr('href', 'javascript:showFullAwardList();');

                setLoading($('#awardPanel'), false);

                selfAward = null;
                for (award of awardData) {
                    if (award.PostedBy === mxUserId) {
                        selfAward = award;
                        break;
                    }
                }

                if (selfAward) {
                    if (new Date() - new Date(parseInt(selfAward.PostedAt.substr(6))) < 60000) {
                        awardButton.html('Undo award');
                    } else {
                        awardButton.addClass('disabled').html('Awarded');
                    }
                } else {
                    awardButton.html('Award');
                }
            });
        }

        function showFullAwardList() {
            $(".hideAwardList").toggle();
            setLoading($('#awardMaster'), true);
            let list = $('<ul style="line-height: 16px;"></ul>');
            for (award of awardData) {
                let li = $('<li></li>');
                li.append(userLink(award.UserLink));
                li.append(',&nbsp;')
                li.append($('<abbr class="awardListTimeEntry" title="' + (new Date(parseInt(award.PostedAt.substr(6)))).toISOString() + '"></abbr>'));
                if (mxIsModerator) {
                    let awardId = award.AwardID;
                    li.append($('<a href="javascript:;">(Delete)</a>').on('click', function () {
                        if (confirm('Delete this award?')) {
                            $.post('/awards/delete/' + awardId, function () {
                                showMessage(true, "Hopefully deleted the award :-)");
                                loadAwards();
                            })
                        }
                    }));
                }
                list.append(li);
            }

            awardListToggle.attr('href', 'javascript:loadAwards();');
            awardList.html('').append(list);

            $('.awardListTimeEntry').timeago();

        }


        awardButton.on('click', function () {
            if (awardData === null || awardButton.hasClass('disabled')) return;

            awardButton.addClass('disabled');
            $.post('/api/awards/' + (selfAward ? 'remove' : 'add') + '/33755', function (data) {
                showMessage(data.Success, data.Message);

                awardButton.removeClass('disabled');
                loadAwards();
            });
        });

             loadAwards();


    </script>

