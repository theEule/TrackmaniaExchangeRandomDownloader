    <script type="text/javascript">
		$(document).ready(function () {
			// Default page setup.
		    /* By default, automatically set the last page for awards and comments */
			pageSelector('TrackCommentsWindow', '/api/tracks/comments/32564/html/', -1);
			mxt.toggleOnlineRecords("tmio-records", "tmio", "dt00gS40OTs0lvTNn3xzq58AtNi");

            $('.dateFormat').each(function (e) {
                var d = new Date($(this).html());
                $(this).html(d.toLocaleString());
            });

		    $('#voteDetailsContainer').hide();
            $('#voteDetailsToggler').html('<i class="icon-collapse"></i> Show rating details');

                        if (window.location.hash) {
                var hash = window.location.hash.substring(1);
                if (hash === 'award') {
                    $.featherlight('/maps/award_map/32564', { contentFilters: 'ajax', type: 'ajax' });
                }
            }
        });

        var cur = 1;

        function switchMPImage(next) {
            var max = 1;
    var date = new Date();
    var curdate = date.getTime();

        if (next) cur++; else cur--;
        if (cur > max) cur = 1;
        else if (cur < 1) cur = max;

            console.log(cur);
        //$('#imgDesc').html('Dimensions: <span id="imgW">(-)</span> px x <span id="imgH">(-)</span> px');
        setTimeout(function () {
            $('#screenshot').attr('src', '/tracks/32564/image/' + cur + '?dummy=' + curdate);
            $('#screenshot-link').attr('data-featherlight', '/maps/imageview/32564/' + cur);
        }, 20);
        //$('#imgLightBox').css('max-height', ($('#imgLightBox').parent().height() - 48) + 'px');
            //$('#imgLightBoxNo').html(cur);
            $('.windowv2-header-subtext > a[id^=image-]').removeClass('active');
            $('#image-' + cur).addClass('active');

        setTimeout(function () {
            var img = document.getElementById('screenshot');
            $('#imgW').html(img.naturalWidth);
            $('#imgH').html(img.naturalHeight);
        }, 300);
    }

        //Copy Track ID feature
        var copyIDBtn = document.querySelector('.js-idcopybtn');
        copyIDBtn.addEventListener('click', function (event) {
            // Select the id link anchor text
            var idLink = document.querySelector('.js-idlink');
            var range = document.createRange();
            range.selectNode(idLink);
            window.getSelection().addRange(range);

            try {
                // Now that we've selected the anchor text, execute the copy command
                var successful = document.execCommand('copy');
                var msg = successful ? 'successful' : 'unsuccessful';
                console.log('Copy id command was ' + msg);
            } catch (err) {
                console.log('Oops, unable to copy');
            }

            // Remove the selections - NOTE: Should use
            // removeRange(range) when it is supported
            window.getSelection().removeAllRanges();
        });

